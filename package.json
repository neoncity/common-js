{
  "name": "@neoncity/common-js",
  "version": "0.0.11",
  "description": "Things shared between packages. Isomorphic, of course.",
  "author": "NeonCity team",
  "scripts": {
    "build": "tsc",
    "pretest": "tsc",
    "test": "mocha lib/tests/index.js",
    "test-with-coverage": "tsc && istanbul cover --config ./.istanbul.yml ./node_modules/mocha/bin/_mocha -- lib/tests/index.js && remap-istanbul -i ./lib/coverage/coverage-final.json -o ./lib/coverage/coverage-remapped.json && coverage-filter -i ./lib/coverage/coverage-remapped.json -o ./lib/coverage/coverage-filtered.json",
    "push-coverage-to-codecov": "codecov -f ./lib/coverage/coverage-filtered.json"
  },
  "keywords": [
    "common"
  ],
  "respository": {
    "type": "git",
    "url": "git+https://github.com/neoncity/common-js.git"
  },
  "private": true,
  "homepage": "https://github.com/neoncity/common-js#readme",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "filesPack": {
    "lib/src": "e:.",
    "README.md": "f:README.md",
    "package.json": "f:package.json",
    "tsconfig.json": "f:tsconfig.json"
  },
  "license": "UNLICENSED",
  "dependencies": {},
  "devDependencies": {
    "@neoncity/coverage-filter": "^0.0.5",
    "@neoncity/togemfury": "^0.0.9",
    "@types/chai": "^3.4.34",
    "@types/istanbul": "^0.4.29",
    "@types/mocha": "^2.2.38",
    "@types/node": "^7.0.0",
    "chai": "^3.5.0",
    "codecov": "^1.0.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "remap-istanbul": "^0.8.4",
    "typescript": "^2.1.5"
  }
}
